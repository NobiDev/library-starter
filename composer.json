{
  "name": "nobidev/library-starter",
  "description": "Library Starter",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "NobiDev",
      "email": "opensource@nobidev.com"
    }
  ],
  "scripts": {
    "test": "phpunit"
  },
  "require": {
    "php": "^7.4|^8.0",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "NobiDev\\LibraryStarter\\": "src/",
      "NobiDev\\LibraryStarter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NobiDev\\LibraryStarter\\ServiceProvider"
      ],
      "aliases": {
        "LibraryStarter": "NobiDev\\LibraryStarter\\Facade"
      }
    }
  }
}
